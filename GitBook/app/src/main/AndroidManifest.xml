<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.GitBook"
        tools:targetApi="31">

        <activity
            android:name=".SignInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnrollActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".ReadActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".MineActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".BookActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".WriteActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".MenuActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".TextActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".WriteMenuActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".WriteTextActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".AskActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".ChangeActivity" android:label="@string/app_name" ></activity>


    </application>


</manifest>